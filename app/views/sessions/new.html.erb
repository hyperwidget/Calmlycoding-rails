<% provide(:title, "Sign in") %>
<h1>Sign in</h1>

    <%= form_for(:session, :url=> sessions_path) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>
      <br/>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <br/>
      <%= f.submit "Sign in" %>
    <% end %>

    <p>Currently not accepting new users, check back later!</p>